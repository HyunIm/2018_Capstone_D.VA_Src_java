<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dva.app_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".MenuActivity"></activity>
        <activity android:name=".ObjectListActivity"></activity>
        <activity android:name=".SearchObjectActivity"></activity>
        <activity android:name=".RobotControlActivity"></activity>
        <activity android:name=".ObjectRegistrationActivity"></activity>
    </application>

</manifest>